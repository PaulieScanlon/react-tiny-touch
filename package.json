{
  "name": "react-tiny-touch",
  "version": "2.0.7",
  "description": "Touch detection in React for adaptive design.",
  "keywords": [
    "React, Touch detection, Adaptive design"
  ],
  "license": "MIT",
  "author": "Paul Scanlon",
  "repository": {
    "type": "git",
    "url": "https://github.com/PaulieScanlon/react-tiny-touch"
  },
  "main": "./dist/umd/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "tslint": "tslint -p tsconfig.json",
    "tslint:fix": "tslint --fix -p tsconfig.json",
    "build": "webpack",
    "prebuild": "tsc",
    "prettier": "prettier --config .prettierrc --write \"**/*.{ts,tsx}\"",
    "storybook": "start-storybook -s ./public -p 8080",
    "storybook:build": "build-storybook -s ./public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tslint && npm run test"
    }
  },
  "dependencies": {
    "typescript": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-syntax-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-info": "^5.0.6",
    "@storybook/addon-knobs": "^5.0.6",
    "@storybook/addon-viewport": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "@storybook/theming": "^5.0.6",
    "@types/enzyme": "^3.9.1",
    "@types/jest": "^24.0.11",
    "@types/node": "^10.12.19",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "@types/react-test-renderer": "^16.8.1",
    "@types/storybook__react": "^4.0.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.2",
    "babel-plugin-react-docgen": "^2.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.4",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-watch-typeahead": "^0.2.1",
    "prettier": "^1.16.4",
    "react": "^16.8.6",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.8.6",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.2.1"
  },
  "bundledDependencies": []
}
